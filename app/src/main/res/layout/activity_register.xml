<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registerVM" type="com.satriasayur.app.modules.register.data.viewmodel.RegisterVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearRegister"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowlogosatriasayur"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyleyellow_200cornerRadius">
            <ImageView android:id="@+id/imageLogoSatriaSayur"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginStart="@dimen/_125pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_logosatriasayur"
                tools:ignore="ContentDescription" tools:src="@drawable/img_logosatriasayur"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSatriaSayur"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_125pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_weight="0.81" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{registerVM.registerModel.txtSatriaSayur}"
                style="@style/txtIntersemibold15" tools:text="@string/lbl_satriasayur"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtDaftarkanDiri"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_33pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{registerVM.registerModel.txtDaftarkanDiri}"
            style="@style/txtInterbold20" tools:text="@string/msg_daftarkan_diri"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtAyomulaiperja"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:text="@{registerVM.registerModel.txtAyomulaiperja}"
            style="@style/txtInterregular13" tools:text="@string/msg_ayo_mulai_perja"/>
        <View android:background="@color/red_700"
            android:id="@+id/lineVectorTwo"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_2pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_300pxh"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnemail"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtEmail"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{registerVM.registerModel.txtEmail}"
                style="@style/txtIntersemibold13" tools:text="@string/lbl_email"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_red_700_radius_20"
                android:drawablePadding="@dimen/_8pxh"
                android:drawableStart="@drawable/img_lock_green_200"
                android:ellipsize="end"
                android:hint="@string/msg_masukkan_email"
                android:id="@+id/etGroup107"
                android:inputType="textEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_15pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={registerVM.registerModel.etGroup107Value}"
                android:textColorHint="@color/green_200"
                style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtKataSandi"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{registerVM.registerModel.txtKataSandi}"
                style="@style/txtIntersemibold13" tools:text="@string/lbl_kata_sandi"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_red_700_radius_20"
                android:drawableEnd="@drawable/img_eye"
                android:drawablePadding="@dimen/_30pxh"
                android:drawableStart="@drawable/img_close"
                android:ellipsize="end"
                android:hint="@string/msg_masukkan_kata_s"
                android:id="@+id/etGroup108"
                android:inputType="textPassword"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_22pxh"
                android:paddingStart="@dimen/_14pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={registerVM.registerModel.etGroup108Value}"
                android:textColorHint="@color/green_200"
                style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtKonfirmasiKata"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{registerVM.registerModel.txtKonfirmasiKata}"
                style="@style/txtIntersemibold13" tools:text="@string/msg_konfirmasi_kata"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_red_700_radius_20"
                android:drawableEnd="@drawable/img_eye"
                android:drawablePadding="@dimen/_30pxh"
                android:drawableStart="@drawable/img_close_green_200"
                android:ellipsize="end"
                android:hint="@string/msg_konfirmasi_kata2"
                android:id="@+id/etGroup109"
                android:inputType="textPassword"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_22pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={registerVM.registerModel.etGroup109Value}"
                android:textColorHint="@color/green_200"
                style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnDaftar"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@string/lbl_daftar"
                android:textAllCaps="false"
                style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_daftar"/>
            <LinearLayout android:id="@+id/linearRowvectorthree"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View android:background="@color/red_700"
                    android:id="@+id/lineVectorThree"
                    android:layout_height="@dimen/_2pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.43" android:layout_width="0dp"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAtau"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_weight="0.13"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{registerVM.registerModel.txtAtau}"
                    style="@style/txtIntersemibold15_1" tools:text="@string/lbl_atau"/>
                <View android:background="@color/red_700"
                    android:id="@+id/lineVectorFour"
                    android:layout_height="@dimen/_2pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.43" android:layout_width="0dp"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowdevicongoogle"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleyellow_2">
                <ImageView android:id="@+id/imageDevicongoogle"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_devicongoogle"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_devicongoogle"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDaftardenganG"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_57pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.89"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{registerVM.registerModel.txtDaftardenganG}"
                    style="@style/txtIntersemibold15_1" tools:text="@string/msg_daftar_dengan_g"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtSudahpunyaaku"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_49pxh"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{registerVM.registerModel.txtSudahpunyaaku}"
                style="@style/txtInterregular13" tools:text="@string/msg_sudah_punya_aku"/>
        </LinearLayout>
    </LinearLayout>
</layout>
