<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="dataDiriVM" type="com.satriasayur.app.modules.datadiri.data.viewmodel.DataDiriVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearDataDiri"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowlogosatriasayur"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyleyellow_200cornerRadius">
            <ImageView android:id="@+id/imageLogoSatriaSayur"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginStart="@dimen/_125pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_logosatriasayur"
                tools:ignore="ContentDescription" tools:src="@drawable/img_logosatriasayur"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSatriaSayur"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_125pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_weight="0.81" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{dataDiriVM.dataDiriModel.txtSatriaSayur}"
                style="@style/txtIntersemibold15" tools:text="@string/lbl_satriasayur"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtIsiDataDiriK"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_33pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{dataDiriVM.dataDiriModel.txtIsiDataDiriK}"
            style="@style/txtInterbold20" tools:text="@string/msg_isi_data_diri_k"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtKamibutuhdata"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:text="@{dataDiriVM.dataDiriModel.txtKamibutuhdata}"
            style="@style/txtInterregular13" tools:text="@string/msg_kami_butuh_data"/>
        <View android:background="@color/red_700"
            android:id="@+id/lineVectorTwo"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_2pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_300pxh"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnnamalengkap"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtNamaLengkap"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{dataDiriVM.dataDiriModel.txtNamaLengkap}"
                style="@style/txtIntersemibold13" tools:text="@string/lbl_nama_lengkap"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_red_700_radius_20"
                android:drawablePadding="@dimen/_9pxh"
                android:drawableStart="@drawable/img_settings"
                android:ellipsize="end"
                android:hint="@string/msg_tulis_nama_leng"
                android:id="@+id/etGroup113" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_15pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@={dataDiriVM.dataDiriModel.etGroup113Value}"
                android:textColorHint="@color/green_200"
                style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTanggalLahir"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{dataDiriVM.dataDiriModel.txtTanggalLahir}"
                style="@style/txtIntersemibold13" tools:text="@string/lbl_tanggal_lahir"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_red_700_radius_20"
                android:drawablePadding="@dimen/_8pxh"
                android:drawableStart="@drawable/img_solarcalendaroutline"
                android:ellipsize="end"
                android:hint="@string/lbl_mm_dd_yyyy"
                android:id="@+id/etGroup115" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_15pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@={dataDiriVM.dataDiriModel.etGroup115Value}"
                android:textColorHint="@color/green_200"
                style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtJenisKelamin"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{dataDiriVM.dataDiriModel.txtJenisKelamin}"
                style="@style/txtIntersemibold13" tools:text="@string/lbl_jenis_kelamin"/>
            <Spinner
                android:background="@drawable/layer_list_bg_white_a700_border_red_700_radius_20"
                android:id="@+id/spinnerGroup116"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerDataDiri"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_40pxh}" tools:itemCount="2" tools:listitem="@layout/row_data_diri"/>
        </LinearLayout>
        <View android:background="@color/red_700"
            android:id="@+id/lineVectorThree"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_2pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_300pxh"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtTujuanDietKes"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{dataDiriVM.dataDiriModel.txtTujuanDietKes}"
            style="@style/txtIntersemibold13" tools:text="@string/msg_tujuan_diet_kes"/>
        <Spinner
            android:background="@drawable/layer_list_bg_white_a700_border_red_700_radius_20"
            android:gravity="center_horizontal"
            android:id="@+id/spinnerGroup114"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnLihatHasilPeriksaSementara"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_50pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_11pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_11pxv" android:singleLine="true"
            android:text="@string/msg_lihat_hasil_per"
            android:textAllCaps="false"
            style="@style/btnSolidRoundedOutline" tools:text="@string/msg_lihat_hasil_per"/>
    </LinearLayout>
</layout>
