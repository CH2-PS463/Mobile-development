<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="scanSayuranVM" type="com.satriasayur.app.modules.scansayuran.data.viewmodel.ScanSayuranVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnmegaphone"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStyleyellow_200cornerRadius">
            <LinearLayout android:id="@+id/linearRowmegaphone"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageMegaphone"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_megaphone"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_megaphone"/>
                <LinearLayout android:id="@+id/linearRowlogosatriasayur"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_93pxh"
                    android:layout_weight="0.93"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLogoSatriaSayur"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_logosatriasayur"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_logosatriasayur"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSatriaSayur"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.68"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{scanSayuranVM.scanSayuranModel.txtSatriaSayur}"
                        style="@style/txtIntersemibold15" tools:text="@string/lbl_satriasayur"/>
                    <ImageView android:id="@+id/imageLock"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_92pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearScanSayuran"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <TextView android:ellipsize="end"
                android:id="@+id/txtScanSayuran"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{scanSayuranVM.scanSayuranModel.txtScanSayuran}"
                style="@style/txtInterbold12" tools:text="@string/lbl_scan_sayuran"/>
            <FrameLayout android:id="@+id/frameLucidescan"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_131pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_27pxv" android:layout_width="@dimen/_131pxh">
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_131pxh"
                    android:layout_width="@dimen/_131pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_red_700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_red_700"/>
                <ImageView android:id="@+id/imageVegetableOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_100pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="@dimen/_100pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_vegetable1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vegetable1"/>
            </FrameLayout>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtGunakanfiturs"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent"
                android:text="@{scanSayuranVM.scanSayuranModel.txtGunakanfiturs}"
                style="@style/txtIntermedium12" tools:text="@string/msg_gunakan_fitur_s"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowcamera"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegreen_2">
                <ImageView android:id="@+id/imageCamera"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginStart="@dimen/_72pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_camera"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_camera"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtScanDariKamer"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_71pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_weight="0.86"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{scanSayuranVM.scanSayuranModel.txtScanDariKamer}"
                    style="@style/txtIntersemibold15_1" tools:text="@string/msg_scan_dari_kamer"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowsolargalleryb"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleyellow_2">
                <ImageView android:id="@+id/imageSolargalleryb"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_70pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_solargalleryb"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_solargalleryb"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUploadDariGal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_70pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_weight="0.85"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{scanSayuranVM.scanSayuranModel.txtUploadDariGal}"
                    style="@style/txtIntersemibold15_1" tools:text="@string/msg_upload_dari_gal"/>
            </LinearLayout>
            <View android:background="@color/teal_800"
                android:id="@+id/lineVectorSeven"
                android:layout_gravity="start"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_301pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtHasilGambar"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{scanSayuranVM.scanSayuranModel.txtHasilGambar}"
                style="@style/txtInterbold12_1" tools:text="@string/lbl_hasil_gambar"/>
            <ImageView android:gravity="start"
                android:id="@+id/imageRectangleSeven"
                android:layout_height="@dimen/_194pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_15pxh}"
                imageRes="@{@drawable/img_rectangle7}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle7"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowtelevision"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegreen_2">
                <ImageView android:id="@+id/imageTelevision"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginStart="@dimen/_69pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_television"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_television"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDeteksiGambar"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_67pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_weight="0.88"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{scanSayuranVM.scanSayuranModel.txtDeteksiGambar}"
                    style="@style/txtIntersemibold15_1" tools:text="@string/msg_deteksi_gambar"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyleyellow_1">
            <FrameLayout android:id="@+id/frameStackrectanglefourteen"
                android:layout_height="@dimen/_49pxv"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_214pxh">
                <View
                    android:background="@drawable/rectangle_bg_green_200_radius_10"
                    android:id="@+id/viewRectangleFourteen"
                    android:layout_height="@dimen/_49pxh" android:layout_width="@dimen/_43pxh"/>
                <LinearLayout android:id="@+id/linearRowgrid"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearColumngrid"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageGrid"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_grid_teal_800"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_grid_teal_800"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtDashboard"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{scanSayuranVM.scanSayuranModel.txtDashboard}"
                            style="@style/txtIntersemibold6" tools:text="@string/lbl_dashboard"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnlockOne"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageLockOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_lock_teal_800"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_lock_teal_800"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtAkunSaya"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{scanSayuranVM.scanSayuranModel.txtAkunSaya}"
                            style="@style/txtIntersemibold6" tools:text="@string/lbl_akun_saya"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearColumnsearch"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageSearch"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_search_teal_800"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_search_teal_800"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtPengaturan"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{scanSayuranVM.scanSayuranModel.txtPengaturan}"
                            style="@style/txtIntersemibold6" tools:text="@string/lbl_pengaturan"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
