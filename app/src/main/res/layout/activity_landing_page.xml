<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="landingPageVM" type="com.satriasayur.app.modules.landingpage.data.viewmodel.LandingPageVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnellipseten"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearLandingPage"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:id="@+id/frameStackellipseten"
                android:layout_height="@dimen/_160pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageEllipseTen"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_160pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse10}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse10"/>
                <LinearLayout
                    android:id="@+id/linearColumnselamatdatang"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_27pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtSelamatDatang"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50pxh"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{landingPageVM.landingPageModel.txtSelamatDatang}"
                        style="@style/txtInterbold15" tools:text="@string/msg_selamat_datang"/>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowlogosatriasayur"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50pxh"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imageLogoSatriaSayur"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_logosatriasayur"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_logosatriasayur"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSatriaSayur"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.82"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{landingPageVM.landingPageModel.txtSatriaSayur}"
                            style="@style/txtIntersemibold20" tools:text="@string/lbl_satriasayur"/>
                    </LinearLayout>
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtBantumembawamu"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:text="@{landingPageVM.landingPageModel.txtBantumembawamu}"
                        style="@style/txtIntersemibold15_1" tools:text="@string/msg_bantu_membawamu"/>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackgroup102"
                android:layout_height="@dimen/_121pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_55pxv"
                android:layout_width="@dimen/_312pxh" style="@style/groupStylegreen_1">
                <ImageView android:id="@+id/imageImageThree"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_109pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_307pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_30pxh}"
                    imageRes="@{@drawable/img_image3}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image3"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtGroup102"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingStart="@dimen/_6pxh"
                    android:paddingTop="@dimen/_3pxv"
                    android:singleLine="true"
                    android:text="@{landingPageVM.landingPageModel.txtGroup102}"
                    style="@style/txtSolidRounded" tools:text="@string/msg_langkah_1_det"/>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackgroup101"
                android:layout_gravity="end"
                android:layout_height="@dimen/_121pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="@dimen/_311pxh" style="@style/groupStylegreen_1">
                <ImageView android:id="@+id/imageImageFour"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_109pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_306pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_30pxh}"
                    imageRes="@{@drawable/img_image4}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image4"/>
                <TextView android:ellipsize="end"
                    android:gravity="center"
                    android:id="@+id/txtGroup101"
                    android:layout_gravity="bottom|end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingEnd="@dimen/_5pxh"
                    android:paddingStart="@dimen/_9pxh"
                    android:paddingTop="@dimen/_3pxv"
                    android:singleLine="true"
                    android:text="@{landingPageVM.landingPageModel.txtGroup101}"
                    style="@style/txtSolidRounded" tools:text="@string/msg_langkah_2_lih"/>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackgroup100"
                android:layout_height="@dimen/_121pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="@dimen/_312pxh" style="@style/groupStylegreen_1">
                <ImageView android:id="@+id/imageImageSix"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_109pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_307pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_30pxh}"
                    imageRes="@{@drawable/img_image6}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image6"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtGroup100"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_1pxv"
                    android:paddingStart="@dimen/_6pxh"
                    android:paddingTop="@dimen/_3pxv"
                    android:singleLine="true"
                    android:text="@{landingPageVM.landingPageModel.txtGroup100}"
                    style="@style/txtSolidRounded" tools:text="@string/msg_langkah_3_bik"/>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackellipseeleven"
                android:layout_height="@dimen/_169pxv"
                android:layout_marginTop="@dimen/_52pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageEllipseEleven"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_169pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse11}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse11"/>
                <EditText
                    android:background="@drawable/rectangle_bg_green_200_border_teal_800_radius_16"
                    android:ellipsize="end"
                    android:hint="@string/lbl_daftar_sekarang"
                    android:id="@+id/etGroupTwentyOne"
                    android:inputType="text"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_99pxh"
                    android:layout_marginStart="@dimen/_99pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_5pxv"
                    android:paddingEnd="@dimen/_21pxh"
                    android:paddingStart="@dimen/_22pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@={landingPageVM.landingPageModel.etGroupTwentyOneValue}"
                    android:textColorHint="@color/teal_800"
                    style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
