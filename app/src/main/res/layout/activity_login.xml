<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginVM" type="com.satriasayur.app.modules.login.data.viewmodel.LoginVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearLogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowlogosatriasayur"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyleyellow_200cornerRadius">
            <ImageView android:id="@+id/imageLogoSatriaSayur"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginStart="@dimen/_125pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_logosatriasayur"
                tools:ignore="ContentDescription" tools:src="@drawable/img_logosatriasayur"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSatriaSayur"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_125pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_weight="0.81" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{loginVM.loginModel.txtSatriaSayur}"
                style="@style/txtIntersemibold15" tools:text="@string/lbl_satriasayur"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtSelamatDatang"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{loginVM.loginModel.txtSelamatDatang}"
            style="@style/txtInterbold20" tools:text="@string/msg_selamat_datang2"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="match_parent"
            android:text="@{loginVM.loginModel.txtLanguage}"
            style="@style/txtInterregular13" tools:text="@string/msg_selamat_datang3"/>
        <View android:background="@color/red_700"
            android:id="@+id/lineVectorTwo"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_2pxv"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_300pxh"/>
        <TextView android:ellipsize="end" android:id="@+id/txtEmail"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{loginVM.loginModel.txtEmail}"
            style="@style/txtIntersemibold13" tools:text="@string/lbl_email"/>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_border_red_700_radius_20"
            android:drawablePadding="@dimen/_8pxh"
            android:drawableStart="@drawable/img_lock_green_200"
            android:ellipsize="end"
            android:hint="@string/msg_masukkan_email"
            android:id="@+id/etGroup122"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_15pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@={loginVM.loginModel.etGroup122Value}"
            android:textColorHint="@color/green_200"
            style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
        <TextView android:ellipsize="end" android:id="@+id/txtKataSandi"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{loginVM.loginModel.txtKataSandi}"
            style="@style/txtIntersemibold13" tools:text="@string/lbl_kata_sandi"/>
        <EditText
            android:background="@drawable/rectangle_bg_white_a700_border_red_700_radius_20"
            android:drawableEnd="@drawable/img_eye"
            android:drawablePadding="@dimen/_30pxh"
            android:drawableStart="@drawable/img_close"
            android:ellipsize="end"
            android:hint="@string/msg_masukkan_kata_s"
            android:id="@+id/etGroup123"
            android:inputType="textPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_22pxh"
            android:paddingStart="@dimen/_14pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@={loginVM.loginModel.etGroup123Value}"
            android:textColorHint="@color/green_200"
            style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLupaKataSandi"
            android:layout_gravity="end"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{loginVM.loginModel.txtLupaKataSandi}"
            style="@style/txtInterregular13_1" textUnderline="true" tools:text="@string/msg_lupa_kata_sandi"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnMasuk"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_104pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_11pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_11pxv" android:singleLine="true"
            android:text="@string/lbl_masuk" android:textAllCaps="false"
            style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_masuk"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowvectorthree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <View android:background="@color/red_700"
                android:id="@+id/lineVectorThree"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_weight="0.43" android:layout_width="0dp"/>
            <TextView android:ellipsize="end" android:id="@+id/txtAtau"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_weight="0.13" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{loginVM.loginModel.txtAtau}"
                style="@style/txtIntersemibold15_1" tools:text="@string/lbl_atau"/>
            <View android:background="@color/red_700"
                android:id="@+id/lineVectorFour"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_weight="0.43" android:layout_width="0dp"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowdevicongoogle"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyleyellow_2">
            <ImageView android:id="@+id/imageDevicongoogle"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginBottom="@dimen/_11pxv"
                android:layout_marginStart="@dimen/_57pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="@dimen/_19pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_devicongoogle"
                tools:ignore="ContentDescription" tools:src="@drawable/img_devicongoogle"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMasukdenganGo"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_56pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_weight="0.89" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{loginVM.loginModel.txtMasukdenganGo}"
                style="@style/txtIntersemibold15_1" tools:text="@string/msg_masuk_dengan_go"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtBelumpunyaaku"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{loginVM.loginModel.txtBelumpunyaaku}"
            style="@style/txtInterregular13" tools:text="@string/msg_belum_punya_aku"/>
    </LinearLayout>
</layout>
